    <artifactId>jhaws-net</artifactId>
    <name>net</name>
    <description>http(s) protocol made simple, build around Apache httpcomponents</description>
    <properties>
        <targetJdk>1.8</targetJdk>
        <project.build.sourceEncoding>utf-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>${project.build.sourceEncoding}</project.reporting.outputEncoding>
        <maven.javadoc.skip>true</maven.javadoc.skip>
        <maven.site.skip>false</maven.site.skip>
        <maven.compiler.optimize>true</maven.compiler.optimize>
        <maven.compiler.fork>true</maven.compiler.fork>
        <maven.compiler.source>${targetJdk}</maven.compiler.source>
        <maven.compiler.target>${targetJdk}</maven.compiler.target>
        <maven.compiler.verbose>true</maven.compiler.verbose>
        <maven.compiler.debug>true</maven.compiler.debug>
        <maven.compiler.debuglevel>lines,vars,source</maven.compiler.debuglevel>
        <maven.javadoc.skip>true</maven.javadoc.skip>
        <maven.test.failure.ignore>true</maven.test.failure.ignore>
        <!-- versions -->
        <version.junit>4.12</version.junit>
        <version.commons.lang3>3.8.1</version.commons.lang3>
        <version.bouncycastle>1.60</version.bouncycastle>
        <version.httpcomponents>4.5.6</version.httpcomponents>
        <version.httpcomponents.nio>4.4.10</version.httpcomponents.nio>
        <version.httpcomponents.cache>4.1.4</version.httpcomponents.cache>
        <version.resteasy>3.0.26.Final</version.resteasy>
        <version.resteasy.jaxrs>3.0.12.Final</version.resteasy.jaxrs>
        <version.xhtmlrenderer>9.1.16</version.xhtmlrenderer>
    </properties>
